{
	"name": "@haitch-ui/monorepo",
	"private": true,
	"type": "module",
	"license": "MIT",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"dev:watch": "turbo run dev:watch --ui=tui",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types",
		"ct": "turbo run check-types",
		"test": "turbo run test",
		"test:watch": "turbo run test:watch",
		"sync:licenses": "node scripts/sync-licenses.mjs",
		"changeset": "changeset",
		"changeset:version": "changeset version",
		"changeset:publish": "changeset publish"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.8",
		"@haitch-ui/configs": "workspace:*",
		"@haitch-ui/react": "workspace:*",
		"@haitch-ui/tests": "workspace:*",
		"@testing-library/jest-dom": "^6.9.1",
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"eslint": "^9.39.1",
		"prettier": "^3.7.4",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"tsup": "^8.5.1",
		"turbo": "^2.7.0",
		"typescript": "5.9.2",
		"vitest": "^4.0.18"
	},
	"overrides": {
		"next": "16.0.10",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"@floating-ui/react": "^0.26.0",
		"vitest": "4.0.18",
		"@vitest/ui": "4.0.18"
	},
	"packageManager": "pnpm@10.0.0",
	"engines": {
		"node": ">=20"
	},
	"workspaces": [
		"apps/*",
		"packages/*",
		"packages/primitives/*",
		".devtools/**",
		"scripts/**"
	]
}
