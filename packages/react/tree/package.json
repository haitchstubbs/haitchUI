		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"@vitejs/plugin-react": "^5.1.2",
		"axe-core": "^4.11.0",
		"jsdom": "^27.3.0",
		"react-dom": "^19.2.0",
		"vitest": "^4.0.16",
		"vitest-axe": "^0.1.0"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
